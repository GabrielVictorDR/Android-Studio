<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.instagram">

    <application
        android:allowBackup="true"
        android:icon="@drawable/instagram_clone_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/instagram_clone_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Instagram">
        <activity android:name=".padrao.CadastroActivityUI" android:theme="@style/Theme.TelaLogin"/>
        <activity android:name=".padrao.MainActivityUI" />
        <activity android:name=".padrao.LoginActivityUI" android:theme="@style/Theme.TelaLogin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>